<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE SCENARIO SYSTEM "sim/multiModelTrafficSimulatorInput.dtd">
<!-- (c) Martin Treiber, Arne Kesting 2010 -->
<SCENARIO>
    <VEHICLES>
        <!-- variants: 0=fullVD orig, 1=fullVD secBased, 2=threePhase -->
        <VEHICLE label="VDIFF1" length="8" b_max="100">
            <LONGITUDINAL_MODEL>
                <OVM_VDIFF v0="15" tau="5" s0="0" l_int="8" beta="1.5"
                    lambda="0.6" variant="0" />
            </LONGITUDINAL_MODEL>
            <LANE_CHANGE_MODEL />
        </VEHICLE>
        <VEHICLE label="Obstacle" length="6" b_max="20">
            <LONGITUDINAL_MODEL>
                <OVM_VDIFF v0="0" tau="5" s0="2" l_int="8" beta="1.5"
                    lambda="0.6" variant="0" />
            </LONGITUDINAL_MODEL>
            <LANE_CHANGE_MODEL />
        </VEHICLE>
    </VEHICLES>
    <SIMULATION dt="0.2" duration="360" fixed_seed="true" seed="42">
	 <TRAFFIC_COMPOSITION write_fund_diagrams="true">
                <VEHICLE_TYPE label="VDIFF1" fraction="1" />
                <VEHICLE_TYPE label="Obstacle" fraction="0" />
            </TRAFFIC_COMPOSITION>
			 <!-- road id 0 indicates mainroad -->    
        <ROAD id="0" length="20000" lanes="1">
            <INITIAL_CONDITIONS>
                <IC_MICRO x="10" v="0" />
                <IC_MICRO x="20" v="0" />
                <IC_MICRO x="30" v="0" />
                <IC_MICRO x="40" v="0" />
                <IC_MICRO x="50" v="0" />
                <IC_MICRO x="60" v="0" />
                <IC_MICRO x="70" v="0" />
                <IC_MICRO x="80" v="0" />
                <IC_MICRO x="90" v="0" />
                <IC_MICRO x="100" v="0" />
                <IC_MICRO x="110" v="0" />
                <IC_MICRO x="120" v="0" />
                <IC_MICRO x="130" v="0" />
                <IC_MICRO x="140" v="0" />
                <IC_MICRO x="150" v="0" />
                <IC_MICRO x="160" v="0" />
                <IC_MICRO x="170" v="0" />
                <IC_MICRO x="180" v="0" />
                <IC_MICRO x="190" v="0" />
                <IC_MICRO x="200" v="0" />
                <IC_MICRO x="210" v="0" />
                <IC_MICRO x="220" v="0" />
                <IC_MICRO x="230" v="0" />
                <IC_MICRO x="240" v="0" />
                <IC_MICRO x="250" v="0" />
                <IC_MICRO x="260" v="0" />
                 <!-- obstacle shifts floating car numbers -->
                <IC_MICRO x="1000" v="0" label="Obstacle" />
            </INITIAL_CONDITIONS>
            <TRAFFIC_SOURCE />
            <TRAFFIC_SINK />
            <FLOW_CONSERVING_INHOMOGENEITIES />
            <SPEED_LIMITS />
            <SIMPLE_RAMPS />
            <TRAFFIC_LIGHTS />
            <DETECTORS />
        </ROAD>
         <OUTPUT>
          <!-- output grid absorbs some information in acceleration -->         
                <FLOATING_CAR_DATA n_dt="5" perc_out="0" dn="0">
                    <FC number="2" />
                    <FC number="6" />
                    <FC number="11" />
                    <FC number="16" />
                    <FC number="21" />
                </FLOATING_CAR_DATA>
            </OUTPUT>
    </SIMULATION>
</SCENARIO>
